<div class="container-fluid">	

	<h1>Create a Deck</h1>
	<div class="row">
		<div class="col-sm-8">
			<div class="panel panel-default">
			  <div class="panel-heading">
			  	<input ng-model="newCard.question" class="form-control" placeholder="Enter question"></div>
			  <div class="panel-body">
			    <input ng-model="newCard.answer" class="form-control" placeholder="Enter answer">
			  </div>
			</div>
		</div>
		<div class="col-sm-4">
			<button type="submit" class="btn btn-primary" ng-click="createCard()">Create Card</button>
		</div>
	</div>



</div>